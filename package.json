{
  "name": "garage-pi",
  "version": "0.0.1",
  "description": "Node.JS Raspberry Pi Garage Door Management Application",
  "main": "server.js",
  "scripts": {
    "start": "npm run server",
    "eslint": "eslint --ext es6 src ",
    "build": "mkdir -p dist/server; cp -va src/server/views dist/server/; cp -va src/static dist; babel src --out-dir dist",
    "server": "npm run eslint && babel-node src/server/server",
    "test": "NODE_ENV=test mocha $(find test -name '*.es6')",
    "server:dev": "nodemon --exec babel-node -- src/server/server.es6",
    "server:prod": "node dist/server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PiGarageDoor/garage-pi.git"
  },
  "keywords": [
    "node",
    "raspberrypi",
    "garage"
  ],
  "author": "PiGarageDoor",
  "contributors": [
    {
      "name": "Vicente Gonz√°lez Monsonis",
      "email": "monsonis@gmail.com"
    },
    {
      "name": "David Rubert",
      "email": "david.rubert@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PiGarageDoor/garage-pi/issues"
  },
  "homepage": "https://github.com/PiGarageDoor/garage-pi#readme",
  "dependencies": {
    "body-parser": "^1.14.2",
    "camo": "^0.11.2",
    "config": "^1.17.1",
    "express": "^4.13.3",
    "jade": "^1.11.0",
    "jsonwebtoken": "^5.5.0",
    "moment": "^2.10.6",
    "morgan": "^1.6.1",
    "nedb": "^1.5.1",
    "onoff": "^1.0.3",
    "qr-image": "^3.1.0"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^5.0.0-beta6",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-plugin-react": "^3.13.1",
    "mocha": "^2.3.4",
    "nodemon": "^1.8.1",
    "webpack": "^1.12.9"
  }
}
