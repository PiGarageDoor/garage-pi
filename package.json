{
  "name": "garage-pi",
  "version": "0.0.1",
  "description": "Node.JS Raspberry Pi Garage Door Management Application",
  "main": "server.js",
  "scripts": {
    "start": "sh entrypoint.sh",
    "init-db": "npm run manage add user data/initial-admin-user.json; npm run manage add door data/initial-door.json",
    "server": "npm run eslint && babel-node src/backend",
    "manage": "babel-node src/tools/manage",
    "test": "NODE_ENV=test mocha $(find test -name '*.es6')",
    "dev": "babel-node -- src/dev-server.es6",
    "dev:full": "nodemon --exec babel-node -- src/dev-server.es6",
    "build": "mkdir -p dist/backend; babel src/server.es6 -o dist/server.js; babel src/backend --out-dir dist/backend; webpack --config config/webpack.production.js --progress --colors",
    "eslint": "eslint --ext es6 --ext jsx src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PiGarageDoor/garage-pi.git"
  },
  "keywords": [
    "node",
    "raspberrypi",
    "garage"
  ],
  "author": "PiGarageDoor",
  "contributors": [
    {
      "name": "Vicente Gonz√°lez Monsonis",
      "email": "monsonis@gmail.com"
    },
    {
      "name": "David Rubert",
      "email": "david.rubert@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PiGarageDoor/garage-pi/issues"
  },
  "homepage": "https://github.com/PiGarageDoor/garage-pi#readme",
  "dependencies": {
    "base-64": "^0.1.0",
    "body-parser": "^1.14.2",
    "camo": "^0.11.2",
    "config": "^1.19.0",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "express-jwt": "^3.3.0",
    "install": "^0.4.2",
    "jsonwebtoken": "^5.5.4",
    "lodash": "^4.3.0",
    "moment": "^2.11.2",
    "morgan": "^1.6.1",
    "nedb": "^1.7.4",
    "npm": "^3.7.1",
    "onoff": "^1.0.3",
    "qr-image": "^3.1.0",
    "spinkit": "^1.2.5"
  },
  "devDependencies": {
    "babel": "^6.5.1",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-eslint": "^5.0.0-beta10",
    "babel-preset-es2015": "^6.5.0",
    "chai": "^3.4.1",
    "commander": "^2.9.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.0",
    "eslint-plugin-react": "^3.16.1",
    "faker": "^3.0.1",
    "material-ui": "^0.14.4",
    "mocha": "^2.4.3",
    "nodemon": "^1.8.1",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "eslint-loader": "^1.2.0",
    "history": "^2.0.0",
    "html-webpack-plugin": "^2.8.1",
    "jwt-decode": "^1.5.1",
    "proxy-middleware": "^0.15.0",
    "react-hot-loader": "^1.3.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-tap-event-plugin": "^0.2.1",
    "redux-actions": "^0.9.1",
    "redux-thunk": "^1.0.3",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0-rc6",
    "redux": "^3.3.1",
    "redux-logger": "^2.5.0",
